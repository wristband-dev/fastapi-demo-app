{
  "name": "fastapi-demo-app--frontend",
  "description": "React frontend client that interacts with the Python FastAPI server and uses the Wristband react-client-auth SDK.",
  "author": "Wristband",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19.2"
  },
  "scripts": {
    "dev": "node scripts/run-dev.js",
    "start": "vite --host",
    "build": "rm -rf dist/; rm -rf ../server/dist/; vite build; mv \"./dist/\" \"../server/dist/\"",
    "serve": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@wristband/react-client-auth": "^2.0.0",
    "axios": "^1.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.10",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
